# RUN: %llvm-crash-analyzer --debug-only=taint-analysis --core-file=%S/Inputs/core.taint-dest-base-reg %S/Inputs/taint-dest-base-reg.out < %s 2>&1 | FileCheck --check-prefix=CHECK-DEBUG %s

